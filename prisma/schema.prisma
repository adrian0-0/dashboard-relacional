// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Professor {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
}

model Empresa {
  id    Int     @id @default(autoincrement())
  programa String  
  name  String
}